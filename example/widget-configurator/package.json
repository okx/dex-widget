{
  "name": "widget-demo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "switch:local": "yalc add @okxweb3/dex-widget && pnpm i",
    "switch:prod": "yalc remove @okxweb3/dex-widget --retreat && pnpm i",
    "push:lib": "cd ../.. && pnpm i  && yalc push",
    "dev:local": "npm run switch:local && npm run push:lib && concurrently \"cd ../.. && vite build --watch\" \"vite\"",
    "dev": "vite",
    "build": "vite build --mode production",
    "build:aladdin": "vite build --mode aladdin --base /assets/dex-iframe-test/",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^5.16.7",
    "@mui/material": "^5.16.7",
    "@okxweb3/dex-widget": "^1.2.1-beta.1",
    "@rainbow-me/rainbowkit": "^2.1.4",
    "@tanstack/react-query": "^5.51.21",
    "@types/lodash-es": "^4.17.12",
    "inter-ui": "^4.0.2",
    "lodash-es": "^4.17.21",
    "mui-color-input": "^3.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-inlinesvg": "^4.1.3",
    "react-syntax-highlighter": "^15.5.0",
    "vite-plugin-node-polyfills": "^0.22.0",
    "wagmi": "^2.12.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@vitejs/plugin-react": "^4.3.1",
    "concurrently": "^8.2.2",
    "eslint": "^9.9.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.4.1",
    "yalc": "1.0.0-pre.53"
  }
}